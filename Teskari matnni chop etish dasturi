#include <iostream>
#include <string>
using namespace std;

int main() {
    string input;
    cout << "Matn kiriting: ";
    getline(cin, input); // Foydalanuvchidan matn olish

    // Teskari matnni chiqarish
    cout << "Teskari matn: ";
    for (int i = input.length() - 1; i >= 0; i--) {
        cout << input[i];
    }
    cout << endl;
    return 0;
}
